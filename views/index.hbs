<h1 class="album-header">
  Albums
</h1>
<ul id="album-list">
  {{#resources.albums.data}}
    <li class="album-item">
      <div class="album-thumb">
        <a href="/albums/{{id}}">
          <img class="album-thumb" src="https://graph.facebook.com/{{id}}/picture">
        </a>
        <div class="album-info">
          <span class="album-name">{{name}}</span>
          <p>{{created_time}}</p>
        </div>
      </div>
    </li>
  {{/resources.albums.data}}
</ul>
